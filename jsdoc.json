{
  "source": {
    "include": ["src"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs|dist)"
  },
  "plugins": ["plugins/typescript"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  }
}
